datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model User {
  id          Int     @id @default(autoincrement())
  gamecode    String
  name        String
}

model Game {
  id Int @id @default(autoincrement())
}

model GamePrompts {
  id       Int    @id @default(autoincrement())
  prompt   String
  response String
  gameId   Int
}
